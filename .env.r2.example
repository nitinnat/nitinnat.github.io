# Cloudflare R2 Configuration
# Copy this file to .env.r2 and fill in your values
# Then run: source .env.r2 before running upload script

# Your R2 endpoint (find this in R2 dashboard after creating API token)
# Format: https://YOUR_ACCOUNT_ID.r2.cloudflarestorage.com
export R2_ENDPOINT=

# Your R2 bucket name
export R2_BUCKET_NAME=blog-assets

# AWS credentials (these are your R2 Access Key ID and Secret Access Key)
export AWS_ACCESS_KEY_ID=
export AWS_SECRET_ACCESS_KEY=
